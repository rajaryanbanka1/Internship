    "def frequency(string):\n",
    "    dict_freq = {}\n",
    "    for i in range(len(string)):\n",
    "        if(string[i] in dict_freq):\n",
    "            dict_freq[string[i]]+=1\n",
    "        else:\n",
    "            dict_freq[string[i]] = 1\n",
    "    return dict_freq\n",
    "\n",
    "String = input()\n",
    "print(frequency(String))"
